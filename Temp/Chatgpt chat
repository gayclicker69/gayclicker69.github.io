index.html
```
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>gayclicker69</title>
  <link rel="stylesheet" type="text/css" href="css/the_gay.css">
</head>
<body>
  <img id="the_gay" src="assets/the_gay.jpg" alt="the gay">
  <p id="score_label">gayness: 0</p>
  <div id="shop">
    <img id="buy_men" src="assets/buy_men.webp" alt="men">
    <p id="buy_men_cost">cost: hi gayness</p>
  </div>
  <script src="js/data.js"></script>
  <script src="js/the_gay.js"></script>
  <script src="js/shop.js"></script>
</body>
</html>
```
js/the_gay.js
```
window.data.unpack();

const the_gay = document.getElementById('the_gay');
const score_label = document.getElementById('score_label');

the_gay.addEventListener('click', function() {
  score += click_power;
  score_label.textContent = `gayness: ${score}`;
  window.data.pack();
});
```
js/data.js
```
// data.js  
window.data = {  
    score: 1,  
    click_power: 1,
    cps: 0,
    men : {
        cost : 100,
        men_num : 0
    },
    unpack() {  
        // Assign to global scope
        window.score = this.score;  
        window.click_power = this.click_power;
        window.men = this.men;
    },  
    pack() {  
        // Update object from globals  
        this.score = window.score;  
        this.click_power = window.click_power;
        this.men = window.men;
    }  
};
```
js/shop.js
```
window.data.unpack();

const buy_men = document.getElementById("buy_men");
const buy_men_label = document.getElementById("buy_men_label");

buy_men.addEventListener('click', function() {
  if (score >= men.cost) {
    score -= men.cost;
    men.num += 1;
    men.cost += 50;
    cps += 1;
  }
  buy_men_label.textContent = `cost: ${men.cost} gayness`;
  window.data.pack();
});
```
